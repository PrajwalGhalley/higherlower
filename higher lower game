   "source": [
    "import random\n",
    "\n",
    "number = random.randint(1,100)\n",
    "guessing = True\n",
    "while guessing:\n",
    "    guess = int(input(\"Guess The Number\"))\n",
    "    if guess < number:\n",
    "        print(\"Guess Too Low\")\n",
    "    if guess > number:\n",
    "        print(\"Guess Too High\")\n",
    "    if guess == number:\n",
    "        guessing = False\n",
    "        print(\"Congrats\")\n",
    "\n"
   ]
